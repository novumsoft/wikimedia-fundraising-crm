DROP TRIGGER IF EXISTS civicrm_contribution_after_delete //
DELIMITER ;

DELIMITER //
CREATE TRIGGER civicrm_contribution_after_delete after delete ON civicrm_contribution FOR EACH ROW BEGIN  
    INSERT INTO wmf_donor (entity_id, last_donation_currency, last_donation_amount, last_donation_usd,
    lifetime_usd_total, last_donation_date, total_2006_2007,is_2006_donor,total_2007_2008,is_2007_donor,total_2008_2009,is_2008_donor,total_2009_2010,is_2009_donor,total_2010_2011,is_2010_donor,total_2011_2012,is_2011_donor,total_2012_2013,is_2012_donor,total_2013_2014,is_2013_donor,total_2014_2015,is_2014_donor,total_2015_2016,is_2015_donor,total_2016_2017,is_2016_donor,total_2017_2018,is_2017_donor,total_2018_2019,is_2018_donor,total_2019_2020,is_2019_donor,total_2020_2021,is_2020_donor)

    SELECT
      OLD.contact_id as entity_id,
      COALESCE(x.original_currency, currency) as last_donation_currency,
      COALESCE(x.original_amount, total_amount, 0) as last_donation_amount,
      COALESCE(total_amount, 0) as last_donation_usd,
      totals.*

    FROM (
      SELECT
        SUM(COALESCE(total_amount, 0)) AS lifetime_usd_total,
        MAX(IF(total_amount > 0, receive_date, NULL)) AS last_donation_date,
     SUM(COALESCE(IF(receive_date BETWEEN '2006-07-01' AND '2007-06-30 23:59:59', c.total_amount, 0),0)) as total_2006_2007,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2006-07-01' AND '2007-06-30 23:59:59', 1, 0)) as is_2006_donor,SUM(COALESCE(IF(receive_date BETWEEN '2007-07-01' AND '2008-06-30 23:59:59', c.total_amount, 0),0)) as total_2007_2008,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2007-07-01' AND '2008-06-30 23:59:59', 1, 0)) as is_2007_donor,SUM(COALESCE(IF(receive_date BETWEEN '2008-07-01' AND '2009-06-30 23:59:59', c.total_amount, 0),0)) as total_2008_2009,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2008-07-01' AND '2009-06-30 23:59:59', 1, 0)) as is_2008_donor,SUM(COALESCE(IF(receive_date BETWEEN '2009-07-01' AND '2010-06-30 23:59:59', c.total_amount, 0),0)) as total_2009_2010,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2009-07-01' AND '2010-06-30 23:59:59', 1, 0)) as is_2009_donor,SUM(COALESCE(IF(receive_date BETWEEN '2010-07-01' AND '2011-06-30 23:59:59', c.total_amount, 0),0)) as total_2010_2011,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2010-07-01' AND '2011-06-30 23:59:59', 1, 0)) as is_2010_donor,SUM(COALESCE(IF(receive_date BETWEEN '2011-07-01' AND '2012-06-30 23:59:59', c.total_amount, 0),0)) as total_2011_2012,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2011-07-01' AND '2012-06-30 23:59:59', 1, 0)) as is_2011_donor,SUM(COALESCE(IF(receive_date BETWEEN '2012-07-01' AND '2013-06-30 23:59:59', c.total_amount, 0),0)) as total_2012_2013,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2012-07-01' AND '2013-06-30 23:59:59', 1, 0)) as is_2012_donor,SUM(COALESCE(IF(receive_date BETWEEN '2013-07-01' AND '2014-06-30 23:59:59', c.total_amount, 0),0)) as total_2013_2014,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2013-07-01' AND '2014-06-30 23:59:59', 1, 0)) as is_2013_donor,SUM(COALESCE(IF(receive_date BETWEEN '2014-07-01' AND '2015-06-30 23:59:59', c.total_amount, 0),0)) as total_2014_2015,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2014-07-01' AND '2015-06-30 23:59:59', 1, 0)) as is_2014_donor,SUM(COALESCE(IF(receive_date BETWEEN '2015-07-01' AND '2016-06-30 23:59:59', c.total_amount, 0),0)) as total_2015_2016,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2015-07-01' AND '2016-06-30 23:59:59', 1, 0)) as is_2015_donor,SUM(COALESCE(IF(receive_date BETWEEN '2016-07-01' AND '2017-06-30 23:59:59', c.total_amount, 0),0)) as total_2016_2017,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2016-07-01' AND '2017-06-30 23:59:59', 1, 0)) as is_2016_donor,SUM(COALESCE(IF(receive_date BETWEEN '2017-07-01' AND '2018-06-30 23:59:59', c.total_amount, 0),0)) as total_2017_2018,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2017-07-01' AND '2018-06-30 23:59:59', 1, 0)) as is_2017_donor,SUM(COALESCE(IF(receive_date BETWEEN '2018-07-01' AND '2019-06-30 23:59:59', c.total_amount, 0),0)) as total_2018_2019,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2018-07-01' AND '2019-06-30 23:59:59', 1, 0)) as is_2018_donor,SUM(COALESCE(IF(receive_date BETWEEN '2019-07-01' AND '2020-06-30 23:59:59', c.total_amount, 0),0)) as total_2019_2020,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2019-07-01' AND '2020-06-30 23:59:59', 1, 0)) as is_2019_donor,SUM(COALESCE(IF(receive_date BETWEEN '2020-07-01' AND '2021-06-30 23:59:59', c.total_amount, 0),0)) as total_2020_2021,MAX(IF(total_amount > 0 AND receive_date BETWEEN '2020-07-01' AND '2021-06-30 23:59:59', 1, 0)) as is_2020_donor
      FROM civicrm_contribution c
      WHERE contact_id = OLD.contact_id
        AND contribution_status_id = 1
    ) as totals
  LEFT JOIN civicrm_contribution cont ON cont.contact_id = OLD.contact_id
  AND receive_date = totals.last_donation_date
  AND contribution_status_id = 1
  AND total_amount > 0
  AND (trxn_id NOT LIKE 'RFD %' OR trxn_id IS NULL)
  LEFT JOIN wmf_contribution_extra x ON x.entity_id = cont.id
  LIMIT 1

  ON DUPLICATE KEY UPDATE
    last_donation_currency = VALUES(last_donation_currency),
    last_donation_amount = VALUES(last_donation_amount),
    last_donation_usd = VALUES(last_donation_usd),
    lifetime_usd_total = VALUES(lifetime_usd_total),
    last_donation_date = VALUES(last_donation_date),
    total_2006_2007 = VALUES(total_2006_2007),is_2006_donor = VALUES(is_2006_donor),total_2007_2008 = VALUES(total_2007_2008),is_2007_donor = VALUES(is_2007_donor),total_2008_2009 = VALUES(total_2008_2009),is_2008_donor = VALUES(is_2008_donor),total_2009_2010 = VALUES(total_2009_2010),is_2009_donor = VALUES(is_2009_donor),total_2010_2011 = VALUES(total_2010_2011),is_2010_donor = VALUES(is_2010_donor),total_2011_2012 = VALUES(total_2011_2012),is_2011_donor = VALUES(is_2011_donor),total_2012_2013 = VALUES(total_2012_2013),is_2012_donor = VALUES(is_2012_donor),total_2013_2014 = VALUES(total_2013_2014),is_2013_donor = VALUES(is_2013_donor),total_2014_2015 = VALUES(total_2014_2015),is_2014_donor = VALUES(is_2014_donor),total_2015_2016 = VALUES(total_2015_2016),is_2015_donor = VALUES(is_2015_donor),total_2016_2017 = VALUES(total_2016_2017),is_2016_donor = VALUES(is_2016_donor),total_2017_2018 = VALUES(total_2017_2018),is_2017_donor = VALUES(is_2017_donor),total_2018_2019 = VALUES(total_2018_2019),is_2018_donor = VALUES(is_2018_donor),total_2019_2020 = VALUES(total_2019_2020),is_2019_donor = VALUES(is_2019_donor),total_2020_2021 = VALUES(total_2020_2021),is_2020_donor = VALUES(is_2020_donor); END //
DELIMITER ;
